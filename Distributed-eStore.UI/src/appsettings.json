{
  "AllowedHosts": "*",
  "app": {
    "name": "ui"
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "",
    "requestRetries": 3
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "api",
    "address": "host.docker.internal",
    "port": "5001",
    "pingEnabled": false,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "restEase": {
    "loadBalancer": "",
    "services": [
      {
        "name": "api-gateway",
        "host": "localhost",
        "scheme": "http",
        "port": "5000"
      }
    ]
  }
}